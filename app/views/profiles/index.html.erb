<p id="notice"><%= notice %></p>

<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Summary</th>
      <th>Work as</th>
      <th>Work in</th>
      <th>Experiences</th>
      <th>Interests</th>
      <th>Skills</th>
      <th>Languages</th>
      <th>Organizations</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.summary %></td>
        <td><%= profile.work_as %></td>
        <td><%= profile.work_in %></td>
        <td><%= profile.experiences %></td>
        <td><%= profile.interests %></td>
        <td><%= profile.skills %></td>
        <td><%= profile.languages %></td>
        <td><%= profile.organizations %></td>
        <td><%= profile.user_id %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
